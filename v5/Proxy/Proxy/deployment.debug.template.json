{
  "$schema-template": "2.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "abbcontainerregistry": {
                "username": "$CONTAINER_REGISTRY_USERNAME_abbcontainerregistry",
                "password": "$CONTAINER_REGISTRY_PASSWORD_abbcontainerregistry",
                "address": "abbcontainerregistry.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "proxymodule": {
            "version": "1.0.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULEDIR<../proxymodule>.debug}",
              "createOptions": {}
            }
          },
          "convertermodule": {
            "version": "1.0.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "abbcontainerregistry.azurecr.io/convertermodule:0.0.1-windows-amd64",
              "createOptions": {}
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "convertermoduleToproxymodule": "FROM /messages/modules/convertermodule/outputs/outputC1 INTO BrokeredEndpoint(\"/modules/proxymodule/inputs/MessageFromConverter\")",
          "proxymoduleToIoTHub": "FROM /messages/modules/proxymodule/outputs/MessageToHub INTO $upstream",
          "proxymoduleToconvertermodule": "FROM /messages/modules/proxymodule/outputs/MessageToConverter INTO BrokeredEndpoint(\"/modules/convertermodule/inputs/MessageFromProxy\")"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}